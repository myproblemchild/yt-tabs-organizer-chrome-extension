{
  "manifest_version": 3,
  "name": "YouTube Tab Organizer",
  "version": "1.0",
  "description": "Sort your YouTube tabs by video durations",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "https://www.youtube.com/*"
  ]
}